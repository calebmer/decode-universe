{
  "private": true,
  "name": "@decode/studio-ui",
  "version": "0.0.0-versionless",
  "main": "./src/index.ts",
  "typings": "./src/index.ts",
  "scripts": {
    "postinstall": "rm -rf node_modules/@decode"
  },
  "dependencies": {
    "@decode/studio-signal-exchange": "file:../studio-signal-exchange",
    "debug": "^2.6.3",
    "immutable": "^3.8.1",
    "react": "^15.4.2",
    "rxjs": "^5.3.0"
  },
  "devDependencies": {
    "@types/debug": "^0.0.29",
    "@types/enzyme": "^2.7.7",
    "@types/jest": "^19.2.2",
    "@types/react": "^15.0.21",
    "enzyme": "^2.8.0",
    "jest": "^19.0.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "ts-jest": "^19.0.8",
    "tslib": "^1.6.0",
    "typescript": "^2.2.2"
  },
  "jest": {
    "automock": true,
    "unmockedModulePathPatterns": [
      "node_modules/ts-jest/.*",
      "node_modules/react/.*",
      "node_modules/enzyme/.*"
    ],
    "transform": {
      ".(js|ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "/__tests__/.*\\.test\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "@decode/studio-signal-exchange(.*)": "<rootDir>/../studio-signal-exchange/src$1"
    }
  }
}
